{
    "version": 2,
    "builds": [
      {
        "src": "face_comparison/face_comparison/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime":"python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
         "distDir":"staricfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "face_comparison/face_comparison/wsgi.py"
      }
    ]
   
  }
